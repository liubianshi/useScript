MAIN	bookmark-path.p6	/^multi MAIN ( Bool :$diff = False) {$/;"	s
MAIN	bookmark-path.p6	/^multi MAIN (:$test) {$/;"	s
MAIN	bookmark-path.p6	/^multi MAIN (Bool :$sync!, Bool :$confirm = False) {$/;"	s
MAIN	bookmark-path.p6	/^multi MAIN (Str:D $path where * !~~\/ ^ <tag> \\: \/) {$/;"	s
